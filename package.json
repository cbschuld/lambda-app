{
  "name": "lambda-app",
  "version": "0.2.0",
  "description": "Lambda App - an opinionated Lambda Application Initialization, Management and Response System.",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "npm run tsc",
    "tsc": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint --ext .js,.ts .",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cbschuld/lambda-app.git"
  },
  "keywords": [
    "lambda",
    "serverless",
    "initialization",
    "bootstrap",
    "logging",
    "http response"
  ],
  "author": "Chris Schuld <cbschuld@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cbschuld/lambda-app/issues"
  },
  "homepage": "https://github.com/cbschuld/lambda-app#readme",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.103",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.16",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "aws-lambda": "^1.0.7",
    "eslint": "^8.23.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^28.1.3",
    "lambda-tree": "^1.0.2",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "ts-multitool": "^0.0.4",
    "uuid-base58": "^1.3.0"
  }
}
